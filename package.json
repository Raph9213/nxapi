    "main": "dist/index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git@gitlab.fancy.org.uk:samuel/nxapi.git"
    },
    "homepage": "https://gitlab.fancy.org.uk/samuel/nxapi",
