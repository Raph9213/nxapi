    "build": {
        "appId": "uk.org.fancy.nxapi.app",
        "productName": "Nintendo Switch Online",
        "copyright": "Copyright Â© 2022 Samuel Elliott",
        "npmRebuild": false,
        "files": [
            "dist/app/bundle",
            "dist/bundle",
            "!dist/app/package",
            "!**/node_modules/**/*",
            "resources/app",
            "resources/common"
        ],
        "asar": false,
        "extraMetadata": {
            "name": "nxapi-app",
            "main": "dist/bundle/app-entry.cjs",
            "dependencies": {},
            "optionalDependencies": {},
            "devDependencies": {}
        },
        "directories": {
            "output": "dist/app/package"
        },
        "protocols": [
            {
                "name": "Nintendo Switch Online",
                "schemes": [
                    "com.nintendo.znca"
                ]
            },
            {
                "name": "Nintendo Account authentication for Nintendo Switch Online",
                "schemes": [
                    "npf71b963c1b7b6d119"
                ]
            },
            {
                "name": "Nintendo Account authentication for Nintendo Switch Parental Controls",
                "schemes": [
                    "npf54789befb391a838"
                ]
            }
        ],
        "mac": {
            "identity": null
        }
    }
