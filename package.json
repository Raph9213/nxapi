        "@types/uuid": "^8.3.4",
        "@types/yargs": "^17.0.14",
        "electron": "^21.3.1",
        "electron-builder": "^23.6.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-native-web": "^0.17.7",
